{
  "exclude": ["**/openapi.json","reference/openapi.json"],
  "formats": {
    "openapi": {
      "rootDir": "reference",
      "include": [
        "**",
        "!**/openapi.json"
      ]
    },
    "json_schema": {
      "rootDir": "models",
      "include": [
        "**"
      ]
    },
    "markdown": {
      "rootDir": "docs"
    },
    "image": {
      "rootDir": "assets/images"
    }
  }
}